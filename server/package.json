{
  "name": "workout-records",
  "version": "1.5.4",
  "description": "Workout Records backend",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "prestart": "npm ci --only=prod",
    "start": "node index.js",
    "production": "NODE_ENV=PROD npm start",
    "lint": "standard && eslint --ignore-path ../.gitignore .",
    "be-test": "mocha 'test/**/**.spec.js' -r chai/register-should",
    "test": "npm run lint -s && npm run be-test -s",
    "test-watch": "nodemon -x 'npm run be-test -s'"
  },
  "repository": {},
  "author": "mikemey",
  "license": "MIT",
  "dependencies": {
    "express": "4.17.1",
    "fs-extra": "10.0.0",
    "moment": "2.29.4",
    "mongodb": "3.6.6",
    "morgan": "1.10.0",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "chai": "4.3.4",
    "eslint": "7.26.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-mocha": "8.1.0",
    "eslint-plugin-standard": "5.0.0",
    "mocha": "8.4.0",
    "nodemon": "2.0.7",
    "standard": "16.0.3",
    "supertest": "6.1.3"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "globals": [
      "$",
      "describe"
    ]
  }
}
